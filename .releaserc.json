{
  "branches": [
    "main",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    },
    {
      "name": "development",
      "prerelease": true
    },
    {
      "name": "1-initialize-repository",
      "prerelease": "rc"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    [
      "@semantic-release/exec",
      {
        "successCmd": "echo \"success=true\" >> \"$GITHUB_OUTPUT\"; echo \"version=${nextRelease.version}\" >> \"$GITHUB_OUTPUT\";",
        "failCmd": "echo \"success=false\" >> \"$GITHUB_OUTPUT\""
      }
    ],
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/exec",
      {
        "prepareCmd": "npx prettier --write CHANGELOG.md"
      }
    ]
  ]
}
